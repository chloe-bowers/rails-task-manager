<h1>Edit Task</h1>

<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed %>
  <%= f.check_box :rating %>
  <%= f.submit %>
<% end %>


<button><%= link_to "Back to tasks", tasks_path %></button>
